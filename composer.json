{
  "name": "dmitryd/typo3-pagepath",
  "type": "typo3-cms-extension",
  "description": "Provides API for Backend modules to get a proper path to the Frontend page (simulateStatic/RealURL/CoolURI-like)",
  "version": "2.0.0",
  "minimum-stability": "beta",
  "authors": [
    {
      "name": "Dmitry Dulepov",
      "email": "dmitry@typo3.org"
    }
  ],
  "require": {
    "php": ">=5.6.0 <6.0",
    "typo3/cms-core": ">=6.2.0,<8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8.0"
  },
  "autoload": {
    "psr-4": {
      "Dmitryd\\Pagepath\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dmitryd\\Pagepath\\Tests\\": "Tests/"
    }
  }
}
